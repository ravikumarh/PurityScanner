@model Admin.Models.clsAttributeMaster
<div class="form-horizontal">

    <div class="form-group">
        <div class="col-md-4">
            @Html.DisplayNameFor(model => model.AttributeName)
        </div>

            <div class="col-md-8">
                @Html.DisplayFor(model => model.AttributeName, new { @class = "form-control" })
            </div>
        </div>
    <div class="form-group">
        <div class="col-md-4">
            @Html.DisplayNameFor(model => model.AttributeImageUrl)
        </div>
        <div class="col-md-8">

            @*@Html.DisplayFor(model => model.AttributeImageUrl, new { @class = "form-control" })*@
            <img id="viewimg" src=@Model.AttributeImageUrl alt="Image not found" width="100px" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-8 col-md-10">
            <input type="button" value="Ok" data-dismiss="modal" class="btn btn-default" />
            <button type="button" data-dismiss="modal" class="btn btn-default">Cancel</button>
        </div>
    </div>


</div>
