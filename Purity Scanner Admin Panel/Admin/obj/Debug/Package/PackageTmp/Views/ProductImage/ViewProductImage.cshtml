@model Admin.Models.clsProductImage

<div class="form-horizontal">
    @Html.HiddenFor(model => model.FilterProductID)
    @Html.HiddenFor(model => model.FilterCountryCode)
    <div class="form-group">
        <div class="col-md-4">
            @Html.LabelFor(model => model.ProductName)
        </div>

        <div class="col-md-8">
            @Html.DisplayFor(model => model.ProductName)
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-4">
            @Html.LabelFor(model => model.CountryName)
        </div>

        <div class="col-md-8">
            @Html.DisplayFor(model => model.CountryName)
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-4">
            @Html.LabelFor(model => model.MoodstockImageKey)
        </div>

        <div class="col-md-8">
            @Html.DisplayFor(model => model.MoodstockImageKey)
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-4">
            @Html.LabelFor(model => model.ProductImageUrl)
        </div>

        <div class="col-md-8">
            @*@Html.DisplayFor(model => model.ProductImageUrl)*@
            <img id="viewimg" src=@Model.ProductImageUrl alt="Image not found" width="100px" class="img-rounded" />
        
        </div>
    </div>
  

    <div class="form-group">
        <div class="col-md-offset-8 col-md-10">
            <input type="button" value="Ok" data-dismiss="modal" class="btn btn-default" />
            <button type="button" data-dismiss="modal" class="btn btn-default">Cancel</button>
        </div>
    </div>
</div>

